{
  "id": null,
  "title": "{{ prefix-name }}-{{ suffix-name }} REST API Service",
  "tags": ["rest", "api", "dotnet"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "HTTP Request Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(http_requests_total[5m])",
          "legendFormat": "RPS"
        }
      ],
      "xAxis": {
        "show": true
      },
      "yAxes": [
        {
          "label": "requests/sec",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "HTTP Response Time",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile"
        },
        {
          "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile"
        }
      ],
      "yAxes": [
        {
          "label": "seconds",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "HTTP Status Codes",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(http_requests_total{status_code=~\"2..\"}[5m])",
          "legendFormat": "2xx Success"
        },
        {
          "expr": "rate(http_requests_total{status_code=~\"4..\"}[5m])",
          "legendFormat": "4xx Client Error"
        },
        {
          "expr": "rate(http_requests_total{status_code=~\"5..\"}[5m])",
          "legendFormat": "5xx Server Error"
        }
      ],
      "yAxes": [
        {
          "label": "requests/sec",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "Database Connection Pool",
      "type": "graph",
      "targets": [
        {
          "expr": "dotnet_connection_pool_connections_total",
          "legendFormat": "Total Connections"
        },
        {
          "expr": "dotnet_connection_pool_connections_active",
          "legendFormat": "Active Connections"
        }
      ],
      "yAxes": [
        {
          "label": "connections",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 5,
      "title": "Memory Usage",
      "type": "graph",
      "targets": [
        {
          "expr": "dotnet_total_memory_bytes / 1024 / 1024",
          "legendFormat": "Memory (MB)"
        }
      ],
      "yAxes": [
        {
          "label": "MB",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 6,
      "title": "GC Collections",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(dotnet_collection_count_total[5m])",
          "legendFormat": "{\{generation}\}"
        }
      ],
      "yAxes": [
        {
          "label": "collections/sec",
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s"
}
